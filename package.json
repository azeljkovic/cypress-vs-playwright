{
  "name": "cypress-vs-playwright",
  "version": "1.0.0",
  "description": "Examples of Cypress and Playwright tests.",
  "main": "index.js",
  "scripts": {
    "cy:run": "npx cypress run --browser chrome --spec 'cypress/e2e/real-world-app'",
    "cy:open": "npx cypress open --browser chrome --e2e",
    "pw:run": "npx playwright test",
    "pw:run-test": "npx playwright test playwright/real-world-app/test.spec.ts --headed",
    "pw:run-headed": "npx playwright test --headed",
    "pw:debug": "PWDEBUG=1 npx playwright test"
  },
  "keywords": [
    "e2e",
    "cypress",
    "playwright"
  ],
  "author": "Aleksandar Zeljkovic",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.23.1",
    "cypress": "^10.4.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
