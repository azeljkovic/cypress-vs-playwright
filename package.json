{
  "name": "cypress-vs-playwright",
  "version": "1.0.0",
  "description": "Examples of Cypress and Playwright tests.",
  "main": "index.js",
  "scripts": {
    "cy:run": "npx cypress run --browser chrome",
    "cy:open": "npx cypress open",
    "cy:open-chrome": "npx cypress open --browser chrome --e2e",
    "pw:run": "npx playwright test",
    "pw:run-headed": "npx playwright test --headed",
    "pw:run-test": "npx playwright test playwright/tests/uiLogin2.spec.ts --headed",
    "pw:debug": "npx playwright test playwright/tests/uiLogin2.spec.ts --debug",
    "pw:run-parallel": "npx playwright test --workers=2",
    "pw:generate-storage-state": "npx playwright test playwright/helpers/auth.spec.ts"
  },
  "keywords": [
    "e2e",
    "cypress",
    "playwright"
  ],
  "author": "Aleksandar Zeljkovic",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "cypress": "^10.9.0",
    "cypress-mochawesome-reporter": "^3.2.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
